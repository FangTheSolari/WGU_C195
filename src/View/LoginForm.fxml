<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <Label fx:id="locationTitleLabel" layoutX="228.0" layoutY="91.0" text="Location:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="locationLabel" alignment="CENTER" layoutX="285.0" layoutY="91.0" prefHeight="20.0" prefWidth="139.0" text="Location Label">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="172.0" layoutY="153.0" text="Username:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="174.0" layoutY="219.0" text="Password:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="usernameText" layoutX="300.0" layoutY="151.0" />
      <TextField fx:id="passwordText" layoutX="300.0" layoutY="216.0" />
      <Button fx:id="loginButton" layoutX="277.0" layoutY="259.0" mnemonicParsing="false" onAction="#loginButClick" text="Login" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="215.0" layoutY="300.0" prefHeight="79.0" prefWidth="170.0" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
